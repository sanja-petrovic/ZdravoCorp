<Window x:Class="ZdravoKlinika.View.DoctorPages.DoctorMainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZdravoKlinika.View.DoctorPages" xmlns:model="clr-namespace:ZdravoKlinika.View.DoctorPages.Model" 
        mc:Ignorable="d"
        Title="DoctorMainWindow" Height="768" Width="1024" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" SizeToContent="WidthAndHeight">
    <Window.Resources>
        <DataTemplate DataType="{x:Type model:DoctorMedicationsViewModel}">
            <local:DoctorMedicationsView/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type model:HomePageViewModel}">
            <local:DoctorHomePage/>
        </DataTemplate>
    </Window.Resources>
    <ContentControl Content="{Binding SelectedVM}" ></ContentControl>
</Window>
