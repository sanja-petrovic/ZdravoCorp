            <Label Content="Raspored" FontSize="14" Padding="50 15 15 15" Foreground="#FF5F5F5F"/>
            <Label Content="Ocenite aplikaciju" FontSize="14" Padding="50 15 15 15" Foreground="#FF5F5F5F"/>
        </StackPanel>
        <Image Margin="26,146,982,606" Source="/Resources/Images/home.png" Stretch="Fill"/>
        <Image Margin="26,196,982,558" Source="/Resources/Images/user.png" Stretch="Fill"/>
        <Image Margin="26,244,982,508" Source="/Resources/Images/stethoscope.png" Stretch="Fill"/>
        <Image Margin="26,342,982,412" Source="/Resources/Images/calendar.png" Stretch="Fill"/>
        <Image Margin="26,392,982,362" Source="/Resources/Images/star.png" Stretch="Fill"/>
        <Image Margin="26,292,982,460" Source="/Resources/Images/medicine.png" Stretch="Fill"/>
        <Ellipse HorizontalAlignment="Left" Height="46" Margin="22,60,0,0" VerticalAlignment="Top" Width="44">
            <Ellipse.Fill>
                <ImageBrush ImageSource="\Resources\Images\b33b530cbd60dbae9ab7f69bbd28ae53.jpg" Stretch="Fill"></ImageBrush>
            </Ellipse.Fill>
        </Ellipse>
        <Label Content="Dr {Doktor}" HorizontalAlignment="Left" Margin="72,58,0,0" VerticalAlignment="Top" FontFamily="Open Sans" FontWeight="DemiBold" FontSize="18"/>
        <Label Content="{}{Title}" Margin="72,82,890,662" Foreground="#FF5F5F5F"/>
        <Viewbox Stretch="Uniform" Margin="313,113,335,149">
            <Calendar Style="{DynamicResource CalendarStyle1}" CalendarButtonStyle="{DynamicResource CalendarCalendarButtonStyle1}" CalendarDayButtonStyle="{DynamicResource CalendarCalendarDayButtonStyle1}" CalendarItemStyle="{DynamicResource CalendarCalendarItemStyle1}"/>
        </Viewbox>
        <Label Content="Mesečni raspored" Margin="313,50,313,658" FontSize="30" FontWeight="SemiBold" FontFamily="OpenSans-Regular"/>
    </Grid>
</Page>
